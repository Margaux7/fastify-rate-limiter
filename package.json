{
  "name": "fastify-rate-limiter",
  "version": "0.1.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Simple rate limiter plugin based on memory and token bucket algorithm.",
  "engines": {
    "node": ">= 12"
  },
  "keywords": [
    "ratelimit",
    "fastify",
    "limit",
    "plugin",
    "token bucket"
  ],
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "~4.28.2",
    "@typescript-eslint/parser": "~4.28.2",
    "eslint": "~7.30.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-jest": "~24.3.6",
    "fastify": "^3.29.0",
    "husky": "^1.2.0",
    "jest": "^28.1.0",
    "lint-staged": "^11.1.2",
    "prettier": "~2.3.2",
    "rimraf": "~3.0.2",
    "ts-jest": "^28.0.0-next.3",
    "tsutils": "~3.21.0",
    "typescript": "~4.3.5"
  },
  "scripts": {
    "serve": "yarn clean && yarn build && concurrently 'yarn build:watch' 'npx nodemon dist/example/server.js'",
    "clean": "rimraf coverage build tmp dist",
    "prebuild": "yarn lint",
    "build:main": "yarn clean && tsc -p tsconfig.prod.json",
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prepublishOnly": "yarn build:main"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "Margaux7 <Margaux7222@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Margaux7/fastify-rate-limiter.git"
  },
  "license": "MIT",
  "dependencies": {
    "fastify-plugin": "^3.0.1",
    "lru-cache": "^7.10.1",
    "tslib": "~2.3.0"
  }
}
